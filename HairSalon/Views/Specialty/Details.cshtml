@using HairSalon.Models;
@using System.Collections.Generic;
@{
  Layout = "_Layout";
}
<h2>Stylists in this Specialty</h2>

@foreach (Stylist stylist in @Model)
    {
    <h2>Stylist:</h2>
    <h3>Name: @stylist.GetStylistName()</h3>
    <h3>Location: @stylist.GetLocation()</h3>
    <h3>Rating: @stylist.GetRating()</h3>
        
    }
    <br>

    <h3><a href="/specialties">Back to Specialties List</a></h3>
